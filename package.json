{
  "name": "defensoria-ms-unidades",
  "version": "1.0.0",
  "description": "Sistema de consulta de unidades da Defensoria PÃºblica de MS",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:all": "npm run test:unit && npm run test:integration",
    "test:auto": "./scripts/test-automation.sh",
    "test:daemon": "./scripts/test-daemon.sh",
    "test:daemon:watch": "./scripts/test-daemon.sh -w tests/",
    "test:daemon:lint": "./scripts/test-daemon.sh -l",
    "lint": "eslint src/ tests/ --quiet",
    "lint:fix": "eslint src/ tests/ --fix",
    "precommit": "npm run test:unit && npm run lint",
    "ci": "npm run test:all && npm run lint"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "keywords": [
    "defensoria",
    "unidades",
    "ms",
    "admin",
    "postgresql"
  ],
  "author": "Pedro Leite",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/server.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["text", "lcov", "html"]
  }
}